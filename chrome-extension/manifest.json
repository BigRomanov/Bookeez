{
  "manifest_version": 2,
  "name": "BookeeZ",
  "version": "0.0.2",

  "description": "Social Bookmarking and Collaborative Learning",

  "permissions": [
    "bookmarks", "storage"
  ],

  "app": {
    "launch": {
      "local_path": "app.html"
    }
  },

  "icons": {
    "128": "icon_128.png",
    "16": "icon_16.png"
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}